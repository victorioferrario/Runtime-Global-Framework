{
  "extends": "eslint:recommended",
  "rules": {
    "quotes": [1, "single"],
    "strict": [2, "global"],
    "eqeqeq": 2,
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "consistent-this": [2, "self"],
    "no-use-before-define": [2, "nofunc"],
    "consistent-return": 0,
    "camelcase": 0,
    "eol-last": 0,
    "no-console": 0
  },
  "globals": {},
  "env":{
    "browser": true,
    "node": true
  }
}
